{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "host": {
      "type": "string",
      "minLength": 1
    },
    "port": {
      "type": "integer",
      "minimum": 0,
      "maximum": 65535
    },
    "user": {
      "type": "string",
      "minLength": 1
    },
    "path": {
      "type": "string",
      "minLength": 1
    },
    "isWindows": {
      "type": "boolean"
    }
  },
  "required": ["host", "port", "user", "path", "isWindows"]
}
