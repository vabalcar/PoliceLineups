{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "defaultEmail": {
      "type": "string",
      "format": "email"
    },
    "defaultFullName": {
      "type": "string",
      "minLength": 1
    },
    "defaultPassword": {
      "type": "string",
      "minLength": 1
    }
  },
  "required": ["defaultEmail", "defaultFullName", "defaultPassword"]
}
